static void hexDump(uint8_t dumpLen, uint8_t *bytePtr) {
  uint8_t working;
  while( dumpLen-- ) {
    working = *bytePtr++;
    Serial.write( hex[ working>>4 ] );
    Serial.write( hex[ working&15 ] );
  }
  //Serial.write('\r');
  //Serial.write('\n');
}

static void binDump(char working) {
  int i;
  for (i=7; i>=0; i--) {
    (bitRead(working,i) == 1) ? Serial.print("1") : Serial.print("0");
  }
}

